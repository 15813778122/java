package com.vince.decorator;

/**
 * 装饰者的基类
 * @author HP
 *
 */

public abstract class Decorator implements Drink{
	private Drink drink;
	public Decorator(Drink drink) {
		this.drink = drink;
	}
	@Override
	public float cost() {
		return drink.cost();
	}

	@Override
	public String description() {
		return drink.description();
	}

}
