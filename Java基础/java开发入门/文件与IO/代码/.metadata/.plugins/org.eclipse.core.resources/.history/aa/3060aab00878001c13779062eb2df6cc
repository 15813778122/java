package com.vince.decorator;

/**
 * 具体的装饰者类
 * @author HP
 *
 */

public class BlackBeanDecorator extends Decorator {

	public BlackBeanDecorator(Drink drink) {
		super(drink);
	}
	@Override
	public float cost() {
		return super.cost()+1.0f;
	}
	@Override
	public String description() {
		return super.description()+"+糖";
	}

}
